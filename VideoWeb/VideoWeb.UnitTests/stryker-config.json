{
  "stryker-config": {
    "reporters": [
      "Progress",
      "ClearText",
      "Html"
    ],
    "log-level": "info",
    "timeout-ms": 10000,
    "log-file": true,
    "project-file": "VideoWeb.csproj",
    "max-concurrent-test-runners": 4,
    "threshold-high": 95,
    "threshold-low": 90,
    "threshold-break": 85,
    "files-to-exclude": [
    ],
    "excluded-mutations": [
    ],
    "ignore-methods": [
      "*LogDebug",
      "*LogTrace",
      "*LogError",
      "*LogInformation",
      "*LogWarning",
      "*AddModelError"
    ],
    "mutate": [
      "!Startup.cs",
      "!Program.cs",
      "!**/ConfigureServicesExtensions.cs",
      "!**/ConfigureAuthSchemeExtensions.cs",
      "!Swagger/AuthResponsesOperationFilter.cs",
      "!Pages/Error.cshtml.cs",
      "!Middleware/*.cs",
      "!Extensions/EnumExtensions.cs",
      "!Helpers/ConferenceHelper.cs",
      "!Mappings/MapperFactory.cs",
      "!**/AssemblyInfo.cs"
    ]
  }
}
