{
  "stryker-config": {
    "reporters": [
      "Progress",
      "ClearText"
    ],
    "verbosity": "error",
    "additional-timeout": 10000,
    "project": "VideoWeb.csproj",
    "concurrency": 4,
    "thresholds.high": 95,
    "thresholds.low": 90,
    "thresholds.break": 85,
    "ignore-mutations": [
    ],
    "ignore-methods": [
      "*LogDebug",
      "*LogTrace",
      "*LogError",
      "*LogInformation",
      "*LogWarning",
      "*AddModelError"
    ],
    "mutate": [
      "!Startup.cs",
      "!Program.cs",
      "!**/ConfigureServicesExtensions.cs",
      "!**/ConfigureAuthSchemeExtensions.cs",
      "!Swagger/AuthResponsesOperationFilter.cs",
      "!Pages/Error.cshtml.cs",
      "!Middleware/*.cs",
      "!Extensions/EnumExtensions.cs",
      "!Helpers/ConferenceHelper.cs",
      "!Mappings/MapperFactory.cs",
      "!**/AssemblyInfo.cs"
    ]
  }
}
