{
  "name": "VideoWeb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "build:ssr": "ng run VideoWeb:server:dev",
    "test": "ng test --code-coverage",
    "test-once": "ng test --watch=false --code-coverage",
    "test-once-ci": "ng test --browsers=ChromeHeadlessNoPrompt --watch=false --code-coverage",
    "lint": "ng lint && npm run prettify",
    "prettify": "run-script-os",
    "prettify:nix": "npx prettier -c 'src/app/**/*.*'",
    "prettify:default": "npx prettier -c \"src\\app\\**\\*.*\"",
    "prettify-fix": "run-script-os",
    "prettify-fix:nix": "npx prettier --write 'src/app/**/*.*'",
    "prettify-fix:default": "npx prettier --write \"src\\app\\**\\*.*\"",
    "prettify-api-client": "run-script-os",
    "prettify-api-client:nix": "npx prettier --write 'src/app/services/clients/api-client.ts'",
    "prettify-api-client:default": "npx prettier --write \"src\\app\\services\\clients\\api-client.ts\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "update-api-client": "nswag run && npm run prettify-api-client"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.13",
    "@angular/common": "^11.2.13",
    "@angular/compiler": "^11.2.13",
    "@angular/core": "^11.2.13",
    "@angular/forms": "^11.2.13",
    "@angular/localize": "^11.2.13",
    "@angular/platform-browser": "^11.2.13",
    "@angular/platform-browser-dynamic": "^11.2.13",
    "@angular/platform-server": "^11.2.13",
    "@angular/router": "^11.2.13",
    "@hmcts/frontend": "^0.0.50-alpha",
    "@microsoft/applicationinsights-web": "^2.6.2",
    "@microsoft/signalr": "^3.1.14",
    "@ng-select/ng-select": "^6.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-auth-oidc-client": "^11.6.8",
    "aspnet-prerendering": "^3.0.1",
    "chart.js": "^2.9.4",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.12.0",
    "govuk-frontend": "^3.11.0",
    "guid-typescript": "^1.0.9",
    "moment": "^2.29.1",
    "ng-bullet": "^1.0.3",
    "ng2-charts": "^2.4.2",
    "ngx-clipboard": "^14.0.1",
    "ngx-device-detector": "^2.0.9",
    "ngx-moment": "^5.0.0",
    "ngx-toastr": "^13.2.1",
    "run-script-os": "^1.1.6",
    "rxjs": "^6.6.3",
    "snyk": "^1.437.3",
    "tslib": "^2.1.0",
    "webrtc-adapter": "^8.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.12",
    "@angular/cli": "^11.2.12",
    "@angular/compiler-cli": "^11.2.13",
    "@angular/language-service": "^11.2.13",
    "@testpossessed/ts-data-builder": "^1.0.1",
    "@types/applicationinsights-js": "^1.0.9",
    "@types/chart.js": "^2.9.29",
