{
  "name": "VideoWeb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c development --port 4201",
    "build": "ng build -c development",
    "build-prod": "ng build --configuration production",
    "build:ssr": "ng run VideoWeb:server:dev",
    "test": "ng test --code-coverage",
    "test-once": "ng test --watch=false --code-coverage",
    "test-once-ci": "ng test --browsers=ChromeHeadlessNoPrompt --watch=false --code-coverage",
    "lint": "ng lint && npm run prettify && npm run translations-lint",
    "lint-fix": "ng lint --fix && npm run prettify-fix",
    "translations-lint": "npx jsonlint-cli src/assets/i18n/*.json",
    "prettify": "run-script-os",
    "prettify:nix": "npx prettier -c 'src/app/**/*.*'",
    "prettify:default": "npx prettier -c \"src\\app\\**\\*.*\"",
    "prettify-fix": "run-script-os",
    "prettify-fix:nix": "npx prettier --write 'src/app/**/*.*'",
    "prettify-fix:default": "npx prettier --write \"src\\app\\**\\*.*\"",
    "prettify-api-client": "run-script-os",
    "prettify-api-client:nix": "npx prettier --write 'src/app/services/clients/api-client.ts'",
    "prettify-api-client:default": "npx prettier --write \"src\\app\\services\\clients\\api-client.ts\"",
    "update-api-client": "nswag run /runtime:Net60 && npm run prettify-fix"
  },
  "private": true,
  "dependencies": {
