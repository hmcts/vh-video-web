{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Information",
        "Microsoft.AspNetCore.SignalR": "Warning",
        "Microsoft.AspNetCore.Http.Connections": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Authority": "https://login.microsoftonline.com/",
    "RedirectUri": "https://localhost:5800/home",
    "PostLogoutRedirectUri": "https://localhost:5800/logout"
  },
  "KinlyConfiguration": {
    "Audience": "kinly.api.endpoints",
    "Issuer": "hmcts.video.hearings.service",
    "ExpiresInMinutes": "480",
    "HashExpiresInMinutes": "30"
  },
  "ConnectionStrings": {
    "RedisCache": "RedisConnectionString",
    "SignalR": "SignalRConnectionString"
  },
  "VhServices": {
    "EventHubPath": "eventhub"
  },
  "ZapScan": false
}
