{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [{
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/VideoWeb/VideoWeb/bin/Debug/netcoreapp3.1/VideoWeb.dll",
            "args": [],
            "cwd": "${workspaceFolder}/VideoWeb/VideoWeb/",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Launch and debug NG",
            "type": "chrome",
            "request": "launch",
            "url": "https://localhost:5800",
            "webRoot": "${workspaceFolder}/VideoWeb/VideoWeb/ClientApp"
        },
    ]
}